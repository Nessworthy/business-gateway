                    </xs:documentation>
                </xs:annotation>
            </xs:element>


            <xs:choice>
                <xs:element name="CautionerParty"
                            type="Q1PartyType" maxOccurs="unbounded" minOccurs="1">
                </xs:element>
                <xs:element name="RegisteredProprietorParty" type="Q1PartyType" maxOccurs="unbounded" minOccurs="1"></xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
